{% extends 'NumaDOAAdminBundle::layout.html.twig' %}

{% block content %}
    <h1>UserGroup</h1>

    <table class="record_properties">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Reg_form_template</th>
            <td>{{ entity.regformtemplate }}</td>
        </tr>
        <tr>
            <th>Description</th>
            <td>{{ entity.description }}</td>
        </tr>
        <tr>
            <th>Immediate_activation</th>
            <td>{{ entity.immediateactivation }}</td>
        </tr>
        <tr>
            <th>User_menu_template</th>
            <td>{{ entity.usermenutemplate }}</td>
        </tr>
        <tr>
            <th>Initial_balance</th>
            <td>{{ entity.initialbalance }}</td>
        </tr>
        <tr>
            <th>Make_user_trusted</th>
            <td>{{ entity.makeusertrusted }}</td>
        </tr>
        </tbody>
    </table>

    <ul class="record_actions">
        <li>
            <a href="{{ path('user_group') }}">
                Back to the list
            </a>
        </li>
        <li>
            <a href="{{ path('user_group_edit', { 'id': entity.id }) }}">
                Edit
            </a>
        </li>
        <li>{{ form(delete_form) }}</li>
    </ul>
{% endblock %}
