    {% extends 'NumaDOADMSBundle::layout.html.twig' %}

{% block content -%}
<div class="box">
    <div class="box-header">
        <h3 class="box-title">Finance list</h3>
    </div>
    <div class="box-body table-responsive no-padding">
        <table class="table table-hover">
        <thead>
            <tr>
                <th>Id</th>
                <th>Dealer_id</th>
                <th>Applicant_type</th>
                <th>Amount_required</th>
                <th>Loan_term</th>
                <th>Down_payment</th>
                <th>Trade_in</th>
                <th>Message_text</th>
                <th>Make</th>
                <th>Model</th>
                <th>Year</th>
                <th>Vehicle_type</th>
                <th>Interested_in</th>
                <th>Cust_name</th>
                <th>Cust_last_name</th>
                <th>Preferred_contact</th>
                <th>Email</th>
                <th>Day_phone</th>
                <th>Cell_phone</th>
                <th>Address</th>
                <th>City</th>
                <th>State</th>
                <th>Zip_code</th>
                <th>Ssn_sin_nr</th>
                <th>Birth_date</th>
                <th>Residence_type</th>
                <th>Monthly_payment</th>
                <th>At_residence</th>
                <th>Employer</th>
                <th>Occupation</th>
                <th>Monthly_income</th>
                <th>On_job</th>
                <th>Business_phone</th>
                <th>Employer_address</th>
                <th>Employer_city</th>
                <th>Employer_state</th>
                <th>Employer_zip</th>
                <th>Source</th>
                <th>Other_monthly_income</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('finance_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.dealerid }}</td>
                <td>{{ entity.applicanttype }}</td>
                <td>{{ entity.amountrequired }}</td>
                <td>{{ entity.loanterm }}</td>
                <td>{{ entity.downpayment }}</td>
                <td>{{ entity.tradein }}</td>
                <td>{{ entity.messagetext }}</td>
                <td>{{ entity.make }}</td>
                <td>{{ entity.model }}</td>
                <td>{{ entity.year }}</td>
                <td>{{ entity.vehicletype }}</td>
                <td>{{ entity.interestedin }}</td>
                <td>{{ entity.custname }}</td>
                <td>{{ entity.custlastname }}</td>
                <td>{{ entity.preferredcontact }}</td>
                <td>{{ entity.email }}</td>
                <td>{{ entity.dayphone }}</td>
                <td>{{ entity.cellphone }}</td>
                <td>{{ entity.address }}</td>
                <td>{{ entity.city }}</td>
                <td>{{ entity.state }}</td>
                <td>{{ entity.zipcode }}</td>
                <td>{{ entity.ssnsinnr }}</td>
                <td>{% if entity.birthdate %}{{ entity.birthdate|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.residencetype }}</td>
                <td>{{ entity.monthlypayment }}</td>
                <td>{% if entity.atresidence %}{{ entity.atresidence|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.employer }}</td>
                <td>{{ entity.occupation }}</td>
                <td>{{ entity.monthlyincome }}</td>
                <td>{% if entity.onjob %}{{ entity.onjob|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.businessphone }}</td>
                <td>{{ entity.employeraddress }}</td>
                <td>{{ entity.employercity }}</td>
                <td>{{ entity.employerstate }}</td>
                <td>{{ entity.employerzip }}</td>
                <td>{{ entity.source }}</td>
                <td>{{ entity.othermonthlyincome }}</td>
                <td><ul>
        <li>
            <a href="{{ path('finance_show', { 'id': entity.id }) }}">show</a>
        </li>
        <li>
            <a href="{{ path('finance_edit', { 'id': entity.id }) }}">edit</a>
        </li>
</ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>

{% endblock %}
