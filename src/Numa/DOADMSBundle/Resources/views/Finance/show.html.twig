{% extends 'NumaDOADMSBundle::layout.html.twig' %}

{% block content %}
<h1>ListingForm</h1>

<div class="nav-tabs-custom">
    <div class="tab-content">
    <h1>Finance</h1>

    <table class="record_properties">
        <tbody>
            {#<tr>#}
                {#<th>Id</th>#}
                {#<td>{{ entity.id }}</td>#}
            {#</tr>#}
            {#<tr>#}
                {#<th>Dealer_id</th>#}
                {#<td>{{ entity.dealerid }}</td>#}
            {#</tr>#}
            <tr>
                <th>Applicant Type</th>
                <td>
                    {% if entity.applicanttype == 0 %}
                        Individual
                    {% else %}
                        Joint
                    {% endif %}
                </td>
            </tr>
            <tr>
                <th>Amount Required</th>
                <td>{{ entity.amountrequired }}</td>
            </tr>
            <tr>
                <th>Loan Term</th>
                <td>{{ entity.loanterm }}</td>
            </tr>
            <tr>
                <th>Down Payment</th>
                <td>{{ entity.downpayment }}</td>
            </tr>
            <tr>
                <th>Trade In</th>
                <td>{{ entity.tradein }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ entity.messagetext }}</td>
            </tr>
            <tr>
                <th>Make</th>
                <td>{{ entity.make }}</td>
            </tr>
            <tr>
                <th>Model</th>
                <td>{{ entity.model }}</td>
            </tr>
            <tr>
                <th>Year</th>
                <td>{{ entity.year }}</td>
            </tr>
            <tr>
                <th>Vehicle Type</th>
                <td>{{ entity.vehicletype }}</td>
            </tr>
            <tr>
                <th>Interested In</th>
                <td>{{ entity.interestedin }}</td>
            </tr>
            <tr>
                <th>Customer</th>
                <td>{{ entity.custname }} {{ entity.custlastname }}</td>
            </tr>
            <tr>
                <th>Preferred Contact</th>
                <td>
                    {% if entity.preferredcontact == 0 %}
                        <i class="glyphicon glyphicon-envelope"></i>
                    {% else %}
                        <i class="glyphicon glyphicon-earphone"></i>
                    {% endif %}
                </td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>Day Phone</th>
                <td>{{ entity.dayphone }}</td>
            </tr>
            <tr>
                <th>Cell Phone</th>
                <td>{{ entity.cellphone }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ entity.address }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ entity.city }}</td>
            </tr>
            <tr>
                <th>State</th>
                <td>{{ entity.state }}</td>
            </tr>
            <tr>
                <th>Zip Code</th>
                <td>{{ entity.zipcode }}</td>
            </tr>
            <tr>
                <th>Ssn Sin Nr</th>
                <td>{{ entity.ssnsinnr }}</td>
            </tr>
            <tr>
                <th>Birth Date</th>
                <td>{{ entity.birthdate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Residence Type</th>
                <td>{{ entity.residencetype }}</td>
            </tr>
            <tr>
                <th>Monthly Payment</th>
                <td>{{ entity.monthlypayment }}</td>
            </tr>
            <tr>
                <th>At Residence</th>
                <td>{{ entity.atresidence|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Employer</th>
                <td>{{ entity.employer }}</td>
            </tr>
            <tr>
                <th>Occupation</th>
                <td>{{ entity.occupation }}</td>
            </tr>
            <tr>
                <th>Monthly Income</th>
                <td>{{ entity.monthlyincome }}</td>
            </tr>
            <tr>
                <th>On Job</th>
                <td>{{ entity.onjob|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Business Phone</th>
                <td>{{ entity.businessphone }}</td>
            </tr>
            <tr>
                <th>Employer Address</th>
                <td>{{ entity.employeraddress }}</td>
            </tr>
            <tr>
                <th>Employer City</th>
                <td>{{ entity.employercity }}</td>
            </tr>
            <tr>
                <th>Employer State</th>
                <td>{{ entity.employerstate }}</td>
            </tr>
            <tr>
                <th>Employer Zip</th>
                <td>{{ entity.employerzip }}</td>
            </tr>
            <tr>
                <th>Source</th>
                <td>{{ entity.source }}</td>
            </tr>
            <tr>
                <th>Other Monthly Income</th>
                <td>{{ entity.othermonthlyincome }}</td>
            </tr>
        </tbody>
    </table>

        
        <a href="{{ path('finance') }}" class="btn btn-primary" >
            Back to the list
        </a>


        {#<a href="{{ path('finance_edit', { 'id': entity.id }) }}" class="btn btn-default">#}
            {#Edit#}
        {#</a>#}
    </div>
</div>
{% endblock %}
