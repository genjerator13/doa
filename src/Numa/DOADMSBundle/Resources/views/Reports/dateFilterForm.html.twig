<div class="controls">
    {#<form class="form-inline" role="form" id="report-form">#}

        {#<div class="input-group col-xs-6">#}
            {#<div class="col-xs-6">#}
                {#<label for="dateFrom">Date From:</label>#}
                {#<input class="datepicker form-control" id="dateFrom" name="dateFrom"#}
                       {#value="{% if startDate is not empty %}{{ startDate|date("Y-m-d") }}{% endif %}">#}
            {#</div>#}
            {#<div class="col-xs-6">#}
                {#<label for="dateTo">Date To:</label>#}
                {#<input class="datepicker form-control" id="dateTo" name="dateTo"#}
                       {#value="{% if startDate is not empty %}{{ endDate|date("Y-m-d") }}{% endif %}">#}
            {#</div>#}
                            {#<span class="input-group-btn">#}
                    {#<button class="btn btn-success" type="submit" name="filter" id="filter-results">Filter</button>#}

                    {#<button class="btn btn-primary" type="submit" name="report" id="create" value="report">Create the Report#}
                    {#</button>#}
                {#</span>#}

        {#</div>#}
        {#<div class="date-text">* Not Required</div>#}
    {#</form>#}
{{ form_start(form) }}
    <div class="input-group col-xs-6">
        <div class="col-xs-6">
            <label for="dateFrom">Date From:</label>
            {{ form_widget(form.dateFrom) }}
        </div>
        <div class="col-xs-6">
            <label for="dateTo">Date To:</label>
            {{ form_widget(form.dateTo) }}
        </div>
    <span class="input-group-btn">
    {#<button class="btn btn-success" type="submit" name="filter" id="filter-results">Filter</button>#}

    {#<button class="btn btn-primary" type="submit" name="report" id="create" value="report">Create the Report#}
        {{ form_widget(form.filter) }}
        {{ form_widget(form.report) }}
    </button>
    </span>

    </div>
    <div class="date-text">* Not Required</div>
{{ form_end(form) }}
</div>
<script>
    $(function () {
        $(".datepicker").datepicker({dateFormat: 'yy-mm-dd'});
    });
</script>
