        <td>Other exp 1</td>
        <td><div style="float:left">{{ sale.otherExp1 }}</div><div style="float:right; margin-right: 35px">{{ sale.gst5 }}</div></td>
        <td>Descrip</td>
        <td>{{ sale.discrip3 }}</td>
        <td>Admin Fees</td>
        <td>{{ sale.adminFees1 }}</td>
