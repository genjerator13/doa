        <td>{{ sale.discrip5 }}</td>
        <td>Protect PKG</td>
        <td>{{ sale.protectPkg1 }}</td>
    </tr>
    <tr>
        <td>Other exp 4</td>
        <td><div style="float:left">{{ sale.otherExp4 }}</div><div style="float:right; margin-right: 35px">{{ sale.gst8 }}</div></td>
