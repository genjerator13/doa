        <td>Glass</td>
        <td><div style="float:left">{{ sale.glass }}</div><div style="float:right; margin-right: 35px">{{ sale.gst2 }}</div></td>
        <td>Descrip</td>
        <td>{{ sale.supplier2 }}</td>
        <td>Warranty</td>
        <td>{{ sale.warranty1 }}</td>
    </tr>
    <tr>
        <td>Mechanical 1</td>
        <td><div style="float:left">{{ sale.mechanical1 }}</div><div style="float:right; margin-right: 35px">{{ sale.gst3 }}</div></td>
        <td>Descrip</td>
        <td>{{ sale.discrip1 }}</td>
        <td>Life Insur</td>
        <td>{{ sale.lifeInsur }}</td>
