<h1>UNIT PROFIT REPORT</h1>
<table cellspacing="0" width="100%" border="0" class="itemsale">
    <tr>
        <td colspan="1">
            Vendor
        </td>
        <td colspan="1">
            {{ sale.vendor }}
        </td>
        <td colspan="1">
            Stock #
        </td>
        <td colspan="1">
            {{ listing.stockNr }}
        </td>
        <td colspan="2">
            &nbsp;
        </td>
    </tr>
    <tr>
        <td colspan="1">
            Sales person
        </td>
        <td colspan="1">
            {{ sale.salesPerson }}
        </td>
        <td colspan="1">
            VIN #
        </td>
        <td colspan="1">
            {{ listing.vin }}
        </td>
        <td colspan="2">
            &nbsp;
        </td>
    </tr>
    <tr>
        <td width="10%">Invoice Nr</td>
        <td width="20%">{{ sale.invoiceNr }}</td>
        <td width="10%"></td>
        <td width="20%"></td>
        <td width="10%"></td>
        <td width="20%"></td>
    </tr>
    <tr>
        <td>Invoice Date</td>
        <td>{{ sale.invoiceDate|date('Y-m-d') }}</td>
        <td width="165px"></td>
        <td width="165px"></td>
        <td width="165px"></td>
        <td width="165px"></td>
    </tr>
    <tr>
        <td>Invoice Amt</td>
        <td>{{ sale.invoiceAmt }}</td>
        <td width="165px"></td>
        <td width="165px"></td>
        <td width="165px"></td>
        <td width="165px"></td>
    </tr>
    <tr>
        <td>Taxes 1</td>
        <td>{{ sale.relatedTaxes1 }}</td>
        <td width="165px"></td>
        <td width="165px"></td>
        <td width="165px"></td>
        <td width="165px"></td>
    </tr>
    <tr>
        <td>Taxes 2</td>
        <td>{{ sale.relatedTaxes2 }}</td>
        <td width="165px"></td>
        <td width="165px"></td>
        <td width="165px"></td>
        <td width="165px"></td>
    </tr>
    <tr>
        <td colspan="2"><h2>UNIT COST RECAP</h2><p style="text-align: right; margin-right: 35px">GST Tax</p></td>
        <td width="165px"></td>
        <td width="165px"></td>
        <td colspan="2"><h2>UNIT REVENUE</h2></td>
    </tr>
    <tr>
        <td>Delivery</td>
        <td><div style="float:left">{{ sale.delivery }}</div><div style="float:right; margin-right: 35px">{{ sale.gst }}</div></td>
        <td>Descrip</td>
        <td>{{ sale.supplier }}</td>
        <td>Sold for</td>
        <td>{{ sale.sellingPrice }}</td>
    </tr>
    <tr>
        <td>Clean Up</td>
        <td><div style="float:left">{{ sale.cleanUp }}</div><div style="float:right; margin-right: 35px">{{ sale.gst1 }}</div></td>
        <td>Descrip</td>
        <td>{{ sale.supplier1 }}</td>
        <td>Trade In</td>
        <td>{{ sale.tradeIn }}</td>
    </tr>
