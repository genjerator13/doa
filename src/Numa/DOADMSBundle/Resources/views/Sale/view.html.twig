        <td>Bank Commis</td>
        <td>{{ sale.bankCommis }}</td>
    </tr>
    <tr>
        <td>Total unit cost</td>
        <td>{{ sale.totalUnitCost }}</td>
        <td width="165px"></td>
        <td width="165px"></td>
        <td>
            {% if sale.other1Des is not null %}
                {{ sale.other1Des|capitalize }}
            {% else %}
                Other 1
            {% endif %}
        </td>
        <td>{{ sale.other1 }}</td>
    </tr>
    <tr>
        <td>Net gain</td>
        <td>{{ sale.netGain }}</td>
        <td colspan="2"></td>
        <td>
            {% if sale.other2Des is not null %}
                {{ sale.other2Des|capitalize }}
            {% else %}
                Other 2
            {% endif %}
        </td>
        <td>{{ sale.other2 }}</td>
    </tr>
