        <td>Descrip</td>
        <td>{{ sale.discrip4 }}</td>
        <td>Doc Fees</td>
        <td>{{ sale.docFees1 }}</td>
    </tr>
    <tr>
        <td>Other exp 3</td>
        <td><div style="float:left">{{ sale.otherExp3 }}</div><div style="float:right; margin-right: 35px">{{ sale.gst7 }}</div></td>
