<?php

namespace Numa\DOAModuleBundle\Entity;

/**
 * PartMoto
 */
class PartMoto
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $product_id;

    /**
     * @var integer
     */
    private $moto_id;

    /**
     * @var integer
     */
    private $year;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set productId
     *
     * @param integer $productId
     *
     * @return PartMoto
     */
    public function setProductId($productId)
    {
        $this->product_id = $productId;

        return $this;
    }

    /**
     * Get productId
     *
     * @return integer
     */
    public function getProductId()
    {
        return $this->product_id;
    }

    /**
     * Set motoId
     *
     * @param integer $motoId
     *
     * @return PartMoto
     */
    public function setMotoId($motoId)
    {
        $this->moto_id = $motoId;

        return $this;
    }

    /**
     * Get motoId
     *
     * @return integer
     */
    public function getMotoId()
    {
        return $this->moto_id;
    }

    /**
     * Set year
     *
     * @param integer $year
     *
     * @return PartMoto
     */
    public function setYear($year)
    {
        $this->year = $year;

        return $this;
    }

    /**
     * Get year
     *
     * @return integer
     */
    public function getYear()
    {
        return $this->year;
    }
}
