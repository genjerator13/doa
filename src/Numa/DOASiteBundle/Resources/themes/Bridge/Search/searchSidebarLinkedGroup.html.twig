{% if group is defined %}
    <div id="makes" class="">
        <div class="panel-group">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <div href="#collapse{{ field }}"
                         data-toggle="collapse">
                        <a class="collapsed">{{ title }}</a>
                        <span class="glyphicon glyphicon-play-circle pull-right" aria-hidden="true"></span>
                    </div>
                </div>
            </div>
        </div>
        <div id="collapse{{ field }}" class="panel-collapse collapse in">
            <div class="panel-body">
                <div class="row">
                    <ul>
                        {% if field=="make_string" %}
                            {% for item in group %}
                                <li><a href="{{ path(route,{'make_string':item['value']}) }}">{{ item['value'] }}
                                        ( {{ item['count'] }} )</a></li>
                            {% endfor %}
                        {% endif %}
                        {% if field=="categorySubType" %}
                            {% for item in group %}
                                <li><a href="{{ path(route,{'categorySubType':item['value']}) }}">{{ item['value'] }}
                                        ( {{ item['count'] }} )</a></li>
                            {% endfor %}
                        {% endif %}
                        {% if field=="year" %}
                            {% for item in group %}
                                <li><a href="{{ path(route,{'year':item['value']}) }}">{{ item['value'] }}
                                        ( {{ item['count'] }} )</a></li>
                            {% endfor %}
                        {% endif %}
                        {% if field=="price" %}
                            {% for item in group %}
                                <li><a href="{{ path("search_all",{'priceFrom':item['from'],'priceTo':item['to']  }) }}">{{ price(item['from'],true)|raw }} - {{ price(item['to'],true)|raw }} ( {{ item.count }}
                                        )</a>
                                </li>
                            {% endfor %}
                        {% endif %}
                    </ul>
                </div>
            </div>
        </div>
    </div>
{% endif %}