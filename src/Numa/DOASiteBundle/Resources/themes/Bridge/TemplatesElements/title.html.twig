<h1 class="title">{{ item.getListingTitle(false,false) }}
    {% if item.getTrim() is not empty %}
        <span class="trim">{{ item.getTrim() }}</span>
            <a class="btn btn-default print pull-right" href="{{ path('item_print_details',{'id':item.id}) }}" target="_blank"><i class="fa fa-print"></i> Print</a>
    {% endif %}
    {% if (item.getSold()==1) %}
        <span class="badge">Sold</span>
    {% endif %}
</h1>