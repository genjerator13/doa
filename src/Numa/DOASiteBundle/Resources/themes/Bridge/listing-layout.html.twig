{% import "NumaDOASiteBundle::components.html.twig" as component %}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    {% include 'NumaDOASiteBundle::favicons.html.twig' %}
    {% include 'NumaDOASiteBundle::metatags.html.twig' %}
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta id="meta" name="viewport" content="width=device-width; initial-scale=1.0"/>

    <title>Dealers on Air</title>
    <meta name="description" content=" " />
    <meta name="keywords" content=" " />
    {% block stylesheets %}
        {% stylesheets '@bootstrap' '@colorbox_css' 'bundles/numadoasite/css/default.css' 'bundles/numadoasite/Bridge/css/bridge.css' 'bundles/numadoasite/Bridge/css/pgwslideshow.css' 'bundles/numadoasite/Bridge/css/style-search.css'  filter='cssrewrite' output='css/*.css' %}
        <link href="{{ asset_url }}" type="text/css" rel="stylesheet"/>
        {% endstylesheets %}
        <link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css" rel="stylesheet">
        <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,700' rel='stylesheet' type='text/css'>
        {{ dealer.metaCSS|raw }}
    {% endblock %}
    {% block javascripts %}
        <script src="//code.jquery.com/jquery-1.11.0.min.js"></script>
        <script src="//code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
        {% javascripts '@bootstrapx_js' '@colorbox_js' 'bundles/numadoasite/Bridge/js/pgwslideshow.js' "bundles/numadoasite/Bridge/js/mobilemenu.js" "bundles/numadoasite/Bridge/js/expanding.js" %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {{ dealer.metaJS|raw }}


        {% endjavascripts %}
        {% include 'NumaDOASiteBundle::ga.html.twig' %}
    {% endblock %}
    {% block ga %}
        {% include "NumaDOASiteBundle::ga.html.twig" %}
    {% endblock %}
    {% include "NumaDOASiteBundle::google_tag_head.html.twig" %}
    {% block fecebook_pixel %}
        {{ include("NumaDOASiteBundle::facebookPixel.html.twig") }}
    {% endblock %}
    {% if dealer.settingCarfax %}
        <script id="carproof-badge-js" type="text/javascript" src="https://badging.carproof.com/Scripts/CarProofBadges.min.js" data-thirdpartykey="{{ dealer.settingCarfax }}"></script>
    {% endif %}
</head>

<body>

{% include "NumaDOASiteBundle::google_tag_body.html.twig" %}
<header class="site-header">
    {% block headline %}
        {% include 'NumaDOASiteBundle::headline.html.twig' %}
    {% endblock %}

    {% block mainmenu %}
        {% include 'NumaDOASiteBundle::mainmenu.html.twig' %}
    {% endblock %}

    {% block mobilemenu %}
        {% include 'NumaDOASiteBundle::mobilemenu.html.twig' %}
    {% endblock %}
</header>
{#<div class="maincontent content-margin">#}
    <div class="container maincontent">
        <div class="alertz dontshow alert"></div>
        {% block content %}

        {% endblock %}
    </div>
{#</div>#}

{% block footer %}
    {% include 'NumaDOASiteBundle::footer.html.twig' %}
{% endblock %}

<script>
    jQuery('a.colorbox').colorbox();
</script>
</body>
</html>