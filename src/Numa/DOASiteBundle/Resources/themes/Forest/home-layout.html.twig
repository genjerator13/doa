{% import "NumaDOASiteBundle::components.html.twig" as component %}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    {% include 'NumaDOASiteBundle::favicons.html.twig' %}
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta id="meta" name="viewport" content="width=device-width; initial-scale=1.0"/>
    <title>Dealers on Air</title>
    <meta name="description" content=" " />
    <meta name="keywords" content=" " />

    {% block stylesheets %}
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap-theme.min.css">

        {% stylesheets 'bundles/numadoasite/Forest/css/template3.css'  filter='cssrewrite' output='css/*.css' %}
        <link href="{{ asset_url }}" type="text/css" rel="stylesheet"/>
        {% endstylesheets %}
        <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,700' rel='stylesheet' type='text/css'>

    {% endblock %}

    {% block javascripts %}
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
        {% javascripts '@bootstrapx_js' "bundles/numadoasite/Mountain/js/mobilemenu.js" %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}

    {% endblock %}
    {% block bodyBackground %}
        {% include "NumaDOASiteBundle::bodyBg.html.twig" %}
    {% endblock %}
    {% block ga %}
        {% include "NumaDOASiteBundle::ga.html.twig" %}
    {% endblock %}
</head>

<body>

<div id="wrapper">

    <div class="header-wrapper">


        {% block header %}
            {% include 'NumaDOASiteBundle::headline.html.twig' %}
        {% endblock %}

        {% block mainmenu %}
            {% include 'NumaDOASiteBundle::mainmenu.html.twig' %}
        {% endblock %}

        {% block mobilemenu %}
            {% include 'NumaDOASiteBundle::mobilemenu.html.twig' %}
        {% endblock %}

    </div>

    <div class="content-wrapper">
        <div class="banner">
            {% block banner %}
            <div class="image">
                {% image 'bundles/numadoasite/Forest/images/banner.jpg' %}
                <img src="{{ asset_url }}"/>
                {% endimage %}
            </div>
            {% endblock %}
            {% block featuredads %}
            {% endblock %}
        </div>

        <div class="button-wrapper">
            {{ displayComponent('forest_button_1','image','page','forest',{"template":"NumaDOASiteBundle::button.html.twig","template_params":{"button":"forest_button_1","num":1}}) }}
            {{ displayComponent('forest_button_2','image','page','forest',{"template":"NumaDOASiteBundle::button.html.twig","template_params":{"button":"forest_button_2","num":1}}) }}
            {{ displayComponent('forest_button_3','image','page','forest',{"template":"NumaDOASiteBundle::button.html.twig","template_params":{"button":"forest_button_3","num":1}}) }}
            {{ displayComponent('forest_button_4','image','page','forest',{"template":"NumaDOASiteBundle::button.html.twig","template_params":{"button":"forest_button_4","num":1}}) }}

        </div>
        {% block content %}
        <div class="content-wrapper">
            {{ displayComponent('page_content',"HTML") }}
        </div>
        {% endblock %}
    </div>
    {% block footer %}
        {% include 'NumaDOASiteBundle::footer.html.twig' %}
    {% endblock %}
{#test#}
</div>
</body>
</html>