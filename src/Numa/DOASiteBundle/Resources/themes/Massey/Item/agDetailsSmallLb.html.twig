<table class="table table-striped">
    <tbody>
    <tr class="">
        <td class="table-col-1">Ag Application:</td>
        <td class="table-col-2" itemprop="bodyType">{{ item.getAgApplication() }}</td>
    </tr>
    <tr class="">
        <td class="table-col-1">Stock #:</td>
        <td class="table-col-2">{{ item.getStockNr() }}</td>
    </tr>
    <tr class="">
        <td class="table-col-1">Type:</td>
        <td class="table-col-2">{{ item.getType() }}</td>
    </tr>
    <tr class="">
        <td class="table-col-1">Mileage:</td>
        <td class="table-col-2">{{ item.getMileage() }}</td>
    </tr>
    <tr class="">
        <td class="table-col-1"># of Hours:</td>
        <td class="table-col-2">{{ item.getOfHours() }}</td>
    </tr>
    </tbody>
</table>
