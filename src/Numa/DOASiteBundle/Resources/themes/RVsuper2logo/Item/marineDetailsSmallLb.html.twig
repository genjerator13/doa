<table class="table table-striped">
    <tbody>
    <tr class="">
        <td class="table-col-1">Boat Type:</td>
        <td class="table-col-2" itemprop="boatType">{{ item.getType() }}</td>
    </tr>
    <tr class="">
        <td class="table-col-1">Stock Number:</td>
        <td class="table-col-2">{{ item.getStockNr() }}</td>
    </tr>
    <tr class="">
        <td class="table-col-1">Hull Design:</td>
        <td class="table-col-2">{{ item.getHullDesign() }}</td>
    </tr>
    <tr class="">
        <td class="table-col-1">Length (ft):</td>
        <td class="table-col-2">{{ item.getLength() }}</td>
    </tr>
    <tr class="">
        <td class="table-col-1">Engine Type:</td>
        <td class="table-col-2">{{ item.getEngine() }}</td>
    </tr>
    </tbody>
</table>
