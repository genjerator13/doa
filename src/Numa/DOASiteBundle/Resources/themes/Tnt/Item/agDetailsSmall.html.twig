{% set liclass="col-md-12 col-xs-12 col-sm-12 col-lg-12" %}
<table class="table table-striped">
    <tbody>
    <tr class="">
        <td class="table-col-1">Ag Application:</td>
        <td class="table-col-2" itemprop="bodyType">{{ item.agApplication }}</td>
        <td class="table-col-1">Stock Number:</td>
        <td class="table-col-2">{{ item.stock_nr }}</td>
    </tr>
    <tr class="">
        <td class="table-col-1">Type:</td>
        <td class="table-col-2">{{ item.type }}</td>
        <td class="table-col-1">Mileage:</td>
        <td class="table-col-2">{{ item.mileage }}</td>
    </tr>
    <tr class="">
        <td class="table-col-1"># of Hours:</td>
        <td class="table-col-2">{{ item.ofHours }}</td>
    </tr>
    </tbody>
</table>