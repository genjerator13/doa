{% set liclass="col-md-12 col-xs-12 col-sm-12 col-lg-12" %}
<table class="table table-striped">
    <tbody>
    <tr class="">
        <td class="table-col-1">Boat Type:</td>
        <td class="table-col-2" itemprop="bodyType">{{ item.type }}</td>
        <td class="table-col-1">Stock Number:</td>
        <td class="table-col-2">{{ item.stock_nr }}</td>
    </tr>
    <tr class="">
        <td class="table-col-1">Hull Design:</td>
        <td class="table-col-2">{{ item.hullDesign }}</td>
        <td class="table-col-1">Length (ft):</td>
        <td class="table-col-2">{{ item.length }}</td>
    </tr>
    <tr class="">
        <td class="table-col-1">Engine Type:</td>
        <td class="table-col-2">{{ item.engine }}</td>
    </tr>
    </tbody>
</table>