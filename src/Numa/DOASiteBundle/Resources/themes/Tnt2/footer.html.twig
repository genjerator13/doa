<footer id="colophon" role="contentinfo">
    <div class="footerFirst">
        <div class="container">
            <div class="row-fluid">
                <asside class="quickLinks col-md-3 clearfix">
                    <h3 class="widget-title">Quick Links</h3>
                    <div class="menu-quick-links-container">
                        {% set bottommenu = displayComponent('bottommenu',"template","dealer") %}
                        {% if bottommenu != '' %}
                            {{ include(template_from_string(bottommenu)) }}
                        {% endif %}
                        {% if bottommenu == '' %}
                            <ul>
                                <li><a href="{{ path("homepage") }}">Home</a></li>
                                <li><a href="{{ path("about_us") }}">About Us</a></li>
                                <li><a href="{{ path("search_dispatch") }}">Products</a></li>
                                <li><a href="{{ path("service_form") }}">Services</a></li>
                                <li><a href="{{ path("contactus") }}">Contact Us</a></li>
                            </ul>
                        {% endif %}
                    </div>
                </asside>
                <asside class="quickLinks col-md-3 clearfix">
                    <h3 class="widget-title">Dealership Hours</h3>
                    <div class="menu-quick-links-container">
                        {% if dealer.ServiceHours is not empty %}
                            {{ dealer.ServiceHours|raw }}
                        {% endif %}
                    </div>
                </asside>
                <asside class="quickLinks  col-md-3  clearfix">
                    <h3 class="widget-title">Location</h3>
                    <div class="menu-quick-links-container">
                        <iframe src="{{ dealer.siteGooglemap() }}" style="border:0" width="100%" height="160"></iframe>
                    </div>
                </asside>
                <asside class="quickLinks  col-md-3 clearfix">
                    <h3 class="widget-title">Contact Us</h3>
                    <div class="menu-quick-links-container contactFooter">
                        <div><a href="{{ dealer.siteGooglemap() }}"><i
                                        class="glyphicon glyphicon-map-marker"></i>{{ dealer.address }}
                                , {{ dealer.city }},{{ dealer.state }},{{ dealer.zip }}</a></div>
                        <div><a href="#"><i class="glyphicon glyphicon-earphone"></i></i>{{ dealer.phone }}</a></div>
                        <div><a href="mailto:{{ dealer.email }}"><i
                                        class="glyphicon glyphicon-envelope"></i>{{ dealer.email }}</a></div>
                    </div>
                </asside>
            </div>
        </div>
    </div>
    <div class="footerSecond">
        <div class="container">
            <div class="copyRight"><p>Copyright &copy; 2017, TNT Auto. <span
                            class="inlineBlock">All Rights Reserved.</span></p>
            </div>
            <div class="dealerLogo"><p><a href="http://www.dmscomplete.com" target="_blank">
                        Powered by DMSCOMPLETE
                    </a></p>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
</footer>