{% import "NumaDOASiteBundle::components.html.twig" as component %}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    {% include 'NumaDOASiteBundle::favicons.html.twig' %}
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta id="meta" name="viewport" content="width=device-width; initial-scale=1.0"/>

    <title>Dealers on Air</title>
    <meta name="description" content=" " />
    <meta name="keywords" content=" " />
    {% block stylesheets %}
        {% stylesheets '@bootstrap' 'bundles/numadoasite/WesternBridge/css/ionicons.min.css' 'bundles/numadoasite/css/default.css' 'bundles/numadoasite/WesternBridge/css/pgwslideshow.css'  'bundles/numadoasite/WesternBridge/css/style-detail.css' 'bundles/numadoasite/WesternBridge/css/style-search.css'   'bundles/numadoasite/WesternBridge/css/bridge.css'  filter='cssrewrite' output='css/*.css' %}
        <link href="{{ asset_url }}" type="text/css" rel="stylesheet"/>
        {% endstylesheets %}
        <link href="https://fonts.googleapis.com/css?family=Oswald|Roboto:300,400,500,700" rel="stylesheet">
        <link href="css/ionicons.css" rel="stylesheet" type="text/css" />
    {% endblock %}
    {% block javascripts %}
        <script src="//code.jquery.com/jquery-1.11.0.min.js"></script>
        <script src="//code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
        {% javascripts '@bootstrapx_js' 'bundles/numadoasite/WesternBridge/js/pgwslideshow.js' "bundles/numadoasite/WesternBridge/js/mobilemenu.js" "bundles/numadoasite/WesternBridge/js/expanding.js" %}
        <script type="text/javascript" src="{{ asset_url }}"></script>

        {% endjavascripts %}
        {% include 'NumaDOASiteBundle::ga.html.twig' %}
    {% endblock %}
    {% block ga %}
        {% include "NumaDOASiteBundle::ga.html.twig" %}
    {% endblock %}
    {% include "NumaDOASiteBundle::google_tag_head.html.twig" %}
    {% block fecebook_pixel %}
        {{ include("NumaDOASiteBundle::facebookPixel.html.twig") }}
    {% endblock %}
</head>

<body>

{% include "NumaDOASiteBundle::google_tag_body.html.twig" %}
<div class="header-wrapper">
    <div class="header clearfix">
        {% block headline %}
            {% include 'NumaDOASiteBundle::headline.html.twig' %}
        {% endblock %}

        {% block mainmenu %}
            <div class="main-menu">
                {% include 'NumaDOASiteBundle::mainmenu.html.twig' %}
            </div>
        {% endblock %}

        {% block mobilemenu %}
            <div class="mobile-menu">
                {% include 'NumaDOASiteBundle::mobilemenu.html.twig' %}
            </div>
        {% endblock %}

    </div>
</div>
<div class="content-wrapper">

    <div class="content">
        {% block content %}
        {% endblock %}
    </div>

</div>
{% include 'NumaDOASiteBundle::footer.html.twig' %}


</body>
</html>