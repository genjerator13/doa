
{% block content %}

{#<h2>{{category.name}}</h2>#}
<div class="list-group id="myTab">
    {% for key,item in catalogs %}
        <a href="{{path('homepage_catalog', {'idcatalog': item.id,'catalog_name':item.name})}}" class="list-group-item">{{item.name}}</a>
                  <div>{{item.description| raw}}
                  <dl class="captions businessCatalogCaptions">
<dt>E-mail:</dt>
<dd>{{item.email}}</dd>
<dt>Website:</dt>
<dd>{{item.url}}</dd>
<dt>Phone:</dt>
<dd>{{item.phone}}</dd>
<dt>Fax:</dt>
<dd>{{item.fax}}</dd>
</dl>
                  </div>

                {%endfor%}
        </div>

{% endblock %}

