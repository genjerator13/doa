{% import "NumaDOASiteBundle::components.html.twig" as component %}
{% set liclass="col-md-6 col-xs-6 col-sm-6 col-lg-6" %}
{{ component.listingField(item.getStatus(),liclass, "Status") }}

    {% if item.getYear() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Year: </span>
            <span class="fieldValue fieldValue-status">{{ item.getYear() }}</span>
        </li>
    {% endif %}
    {% if item.getMake() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Make Model: </span>
            <span class="fieldValue fieldValue-status">{{ item.getMake() }}</span>
        </li>
    {% endif %}
    {% if item.getModel() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Model: </span>
            <span class="fieldValue fieldValue-status">{{ item.getModel() }}</span>
        </li>
    {% endif %}
    {% if item.getStockNr() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Stock Number: </span>
            <span class="fieldValue fieldValue-status">{{ item.getStockNr() }}</span>
        </li>
    {% endif %}
    {% if item.getAgApplication() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Ag Application: </span>
            <span class="fieldValue fieldValue-status">{{ item.getAgApplication() }}</span>
        </li>
    {% endif %}
    {% if item.getType() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Type: </span>
            <span class="fieldValue fieldValue-status">{{ item.getType() }}</span>
        </li>
    {% endif %}
    {% if item.getMileage() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Mileage: </span>
            <span class="fieldValue fieldValue-status">{{ item.getMileage() }}</span>
        </li>
    {% endif %}
    {% if item.getPrice() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Price: </span>
            <span class="fieldValue fieldValue-status">{{ item.getPriceString() }}</span>
        </li>
    {% endif %}
    {% if item.getDriveType() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Drive Type: </span>
            <span class="fieldValue fieldValue-status">{{ item.getDriveType() }}</span>
        </li>
    {% endif %}
    {% if item.getEngine() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Engine: </span>
            <span class="fieldValue fieldValue-status">{{ item.getEngine() }}</span>
        </li>
    {% endif %}
    {% if item.getHorsepower() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Horsepower: </span>
            <span class="fieldValue fieldValue-status">{{ item.getHorsepower() }}</span>
        </li>
    {% endif %}
    {% if item.getOfHours() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Hours: </span>
            <span class="fieldValue fieldValue-status">{{ item.getOfHours() }}</span>

        </li>
    {% endif %}
    {% if item.getFuelType() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Fuel Type: </span>
            <span class="fieldValue fieldValue-status">{{ item.getFuelType() }}</span>
        </li>
    {% endif %}
    {% if item.getTransmission() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Transmission: </span>
            <span class="fieldValue fieldValue-status">{{ item.getTransmission() }}</span>
        </li>
    {% endif %}
    {% if item.getOperatorStation() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Operator Station: </span>
            <span class="fieldValue fieldValue-status">{{ item.getOperatorStation() }}</span>
        </li>
    {% endif %}
    {% if item.getSteering() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Steering: </span>
            <span class="fieldValue fieldValue-status">{{ item.getSteering() }}</span>
        </li>
    {% endif %}
    {% if item.getSpeedForward() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Speed Forward: </span>
            <span class="fieldValue fieldValue-status">{{ item.getSpeedForward() }}</span>
        </li>
    {% endif %}
    {% if item.getSpeedReverse() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Speed Reverse: </span>
            <span class="fieldValue fieldValue-status">{{ item.getSpeedReverse() }}</span>
        </li>
    {% endif %}
    {% if item.getTireSize() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Tire Size: </span>
            <span class="fieldValue fieldValue-status">{{ item.getTireSize() }}</span>
        </li>
    {% endif %}
    {% if item.getTireSize() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Tire Equipment: </span>
            <span class="fieldValue fieldValue-status">{{ item.getTireSize() }}</span>
        </li>
    {% endif %}
    {% if item.getCuttingWidth() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Cutting Width: </span>
            <span class="fieldValue fieldValue-status">{{ item.getCuttingWidth() }}</span>
        </li>
    {% endif %}
    {% if item.getExteriorColor() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field">Exterior Color: </span>
            <span class="fieldValue fieldValue">{{ item.getExteriorColor()|raw }}</span>
        </li>
    {% endif %}
    {% if item.getOther() is not empty %}
        <li class="col-md-12 col-xs-12 col-sm-12 col-lg-12">
            &nbsp;
        </li>
        <li class="col-md-12 col-xs-12 col-sm-12 col-lg-12">
            <span class="fieldCaption field">Other: </span>
            <span class="fieldValue fieldValue">{{ item.getOther()|raw }}</span>
        </li>
    {% endif %}
    {% if item.getSellerComment() is not empty %}
        <li class="col-md-12 col-xs-12 col-sm-12 col-lg-12">
            &nbsp;
        </li>
        <li class="col-md-12 col-xs-12 col-sm-12 col-lg-12">
            <div class="fieldCaption field-status">Seller Comments:</div>
            <span class="fieldValue fieldValue-status">
                {% if item.sellerCommentActive == 0 %}
                    {{ item.getSellerComment()|raw }}
                {% elseif item.sellerCommentActive == 1 %}
                    {{ item.getSellerComment1()|raw }}
                {% elseif item.sellerCommentActive == 2 %}
                    {{ item.getSellerComment2()|raw }}
                {% endif %}
            </span>
        </li>
    {% endif %}
