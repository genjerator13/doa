{% set liclass="col-md-12 col-xs-12 col-sm-12 col-lg-12" %}

    {% if item.agApplication is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Ag Application: </span>
            <span class="fieldValue fieldValue-status">{{ item.agApplication }}</span>
        </li>
    {% endif %}
    {% if item.stock_nr is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Stock Number: </span>
            <span class="fieldValue fieldValue-status">{{ item.stock_nr }}</span>
        </li>
    {% endif %}
    {% if item.type() is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Type: </span>
            <span class="fieldValue fieldValue-status">{{ item.type }}</span>
        </li>
    {% endif %}
    {% if item.mileage is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status">Mileage: </span>
            <span class="fieldValue fieldValue-status">{{ item.mileage }}</span>
        </li>
    {% endif %}
    {% if item.ofHours is not empty %}
        <li class="{{ liclass }}">
            <span class="fieldCaption field-status"># of Hours: </span>
            <span class="fieldValue fieldValue-status">{{ item.ofHours }}</span>
        </li>
    {% endif %}
