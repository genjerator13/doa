<h4>Details:</h4>
<ul class="techData">
    <li class="col-md-6">
        <span class="fieldCaption field-status">Status: </span>
        <span class="fieldValue fieldValue-status">{{item.getStatus()}}</span>
    </li>
    <li class="col-md-6">
        <span class="fieldCaption field-status">Year: </span>
        <span class="fieldValue fieldValue-status">{{item.getYear()}}</span>
    </li>
    <li class="col-md-6">
        <span class="fieldCaption field-status">Make Model: </span>
        <span class="fieldValue fieldValue-status">{{item.getMake()}}</span>
    </li>

    <li class="col-md-6">
        <span class="fieldCaption field-status">Model: </span>
        <span class="fieldValue fieldValue-status">{{item.getModel()}}</span>
    </li>            
    <li class="col-md-6">
        <span class="fieldCaption field-status">Boat Type: </span>
        <span class="fieldValue fieldValue-status">{{item.getType()}}</span>
    </li>
    <li class="col-md-6">
        <span class="fieldCaption field-status">Length (ft): </span>
        <span class="fieldValue fieldValue-status">{{item.getLength()}}</span>
    </li>
    <li class="col-md-6">
        <span class="fieldCaption field-status">Beam: </span>
        <span class="fieldValue fieldValue-status">{{item.getBeam()}}</span>
    </li>
    <li class="col-md-6">
        <span class="fieldCaption field-status">Weight: </span>
        <span class="fieldValue fieldValue-status">{{item.getWeight()}}</span>
    </li>
    <li class="col-md-6">
        <span class="fieldCaption field-status">Hull Design: </span>
        <span class="fieldValue fieldValue-status">{{item.getHullDesign()}}</span>
    </li>
    <li class="col-md-6">
        <span class="fieldCaption field-status">Steering Type: </span>
        <span class="fieldValue fieldValue-status">{{item.getSteeringType()}}</span>
    </li>
    <li class="col-md-6">
        <span class="fieldCaption field-status">Drive Type: </span>
        <span class="fieldValue fieldValue-status">{{item.getDriveType()}}</span>
    </li>
    <li class="col-md-6">
        <span class="fieldCaption field-status">Engine Type: </span>
        <span class="fieldValue fieldValue-status">{{item.getEngineType()}}</span>
    </li>
    <li class="col-md-6">
        <span class="fieldCaption field-status">Horsepower: </span>
        <span class="fieldValue fieldValue-status">{{item.getHorsepower()}}</span>
    </li>
    </li>
        <li class="col-md-6">
        <span class="fieldCaption field-status">Fuel Type: </span>
        <span class="fieldValue fieldValue-status">{{item.getFuelType()}}</span>
    </li>
    </li>
        <li class="col-md-6">
        <span class="fieldCaption field-status">Fuel Capacity: </span>
        <span class="fieldValue fieldValue-status">{{item.getFuelCapacity()}}</span>
    </li>    
    </li>
        <li class="col-md-6">
        <span class="fieldCaption field-status"># of Hours: </span>
        <span class="fieldValue fieldValue-status">{{item.getOfHours()}}</span>
    </li>    
    </li>
        <li class="col-md-6">
        <span class="fieldCaption field-status">Transmission: </span>
        <span class="fieldValue fieldValue-status">{{item.getTransmission()}}</span>
    </li>    
    </li>
        <li class="col-md-6">
        <span class="fieldCaption field-status">Exterior Color: </span>
        <span class="fieldValue fieldValue-status">{{item.getExteriorColor()}}</span>
    </li>    
    </li>
        <li class="col-md-6">
        <span class="fieldCaption field-status">Interior Color: </span>
        <span class="fieldValue fieldValue-status">{{item.getInteriorColor()}}</span>
    </li>    
    </li>
        <li class="col-md-6">
        <span class="fieldCaption field-status">Passengers: </span>
        <span class="fieldValue fieldValue-status">{{item.getPassengers()}}</span>
    </li>    
    </li>
        <li class="col-md-6">
        <span class="fieldCaption field-status">Trailer</span>
        <span class="fieldValue fieldValue-status">{{item.getTrailer()}}</span>
    </li>    
    </li>
        <li class="col-md-6">
        <span class="fieldCaption field-status">Battery: </span>
        <span class="fieldValue fieldValue-status">{{item.getInteriorColor()}}</span>
    </li>    
   

    {% if item.getSellerComment() is not empty %}
        <li class="col-md-12">
            <div class="fieldCaption field" >Seller Comments: </div>
            <span class="fieldValue fieldValue">{{item.getSellerComment()}}</span>
        </li>
    {% endif %}
</ul>
