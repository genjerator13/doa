{% if items is defined and items is not empty %}
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">Featured Ads</h3>
        </div>
        <div class="panel-body">
            {% for item in items %}
                {% set images = item.getImages() %}
                {% set desc = item.getUrlDescription() %}
                <a href="{{ path('item_details', {'dealer':'aaa','itemId': item.id, 'description': desc}) }}">{{ item.getItemFieldByName('year') }} {{ item.getMake() }} {{ item.getModel() }}</br>


                </a>
            {% endfor %}
        </div>
    </div>
{% endif %}