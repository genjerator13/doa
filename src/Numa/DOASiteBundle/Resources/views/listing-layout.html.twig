<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<!-- default:main:default.tpl -->
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
    <head>
        <meta name="keywords" content="" />
        <meta name="description" content="" />
        {% block title %}<title>{{title}}</title>{% endblock title %}
        {% block stylesheets %}
            {% stylesheets '@bootstrap'  '@jquery_ui_css' '@flex_slider_css' '@pgw_slideshow_css' '@style' filter='cssrewrite' output='css/*.css' %}
            <link href="{{ asset_url }}" type="text/css" rel="stylesheet" />
            {% endstylesheets %}
        {% endblock %}
        {% block javascripts %}
            <script src="//code.jquery.com/jquery-1.11.0.min.js"></script>
            <script src="//code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
            {% javascripts '@bootstrapx_js' '@jquery_ui_js' '@common_js' '@flex_slider_js' '@pgw_slideshow_js'%}
            <script type="text/javascript" src="{{ asset_url }}"></script>

            {% endjavascripts %}
            {% include 'NumaDOASiteBundle::ga.html.twig' %}
        {% endblock %}
        {% block canonical %}<link rel="canonical" href="{{ url }}" />{% endblock canonical %}
    </head>
    <body class=" default ">
        <!-- Static navbar -->
        <div class="navbar navbar-default navbar-static-top" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                </div>
                <div class="navbar-collapse collapse">         
                    {% set route = app.request.get('_route') %}
                        {{ render_esi(controller('NumaDOASiteBundle:Default:search',{'route':route})) }}
                    </div>
                </div>
            </div>
            <div class="header-background" >

            </div>
            <div class="container">
                <div class="header-background-content" >
                    <div class="header-image" >
                        <div class="header-logo" >

                        </div>
                    </div>

                </div>

                <div class="row">
                    <div class="col-md-2 col-xs-2 col-sm-2 col-lg-2 ">
                        {{ render_esi(controller('NumaDOASiteBundle:Default:sidebarMenu')) }} 
                        {{ render_esi(controller('NumaDOASiteBundle:Default:featuredAdd', { 'max': 5 })) }} 
                    </div>
                    <div class="col-md-8 col-xs-8 col-sm-8 col-lg-8">
                        {% block content %}
                        {% endblock %}
                    </div>
                    <div class="col-md-2 col-xs-2 col-sm-2 col-lg-2 right-sidebar" >
                        {% block rightsidebar %}
                        {% endblock %}
                    </div>
                </div>
                <div class="row">
                    <div class="footer col-md-12 col-xs-12 col-sm-12 col-lg-12">
                        <a class="footer-img"></a>
                        {% include 'NumaDOASiteBundle::copyright.html.twig' %}
                    </div>        
                </div>  
            </div> <!-- /container -->
        </body>
    </html>
