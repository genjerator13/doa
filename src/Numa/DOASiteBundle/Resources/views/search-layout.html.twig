<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<!-- default:main:default.tpl -->
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
    {% include 'NumaDOASiteBundle::header.html.twig'  %}

    <body class=" default ">
        <!-- Static navbar -->
        <div class="navbar navbar-default navbar-static-top" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                </div>
                <div class="navbar-collapse collapse">
                    {% set route = app.request.get('_route') %}
                        {{ render_esi(controller('NumaDOASiteBundle:Default:search',{'route':route})) }}
                    </div><!--/.nav-collapse -->
                </div>
            </div>
            <div class="header-background" >

            </div>
            <div class="container">
                <div class="header-background-content" >
                    <div class="header-image">
                        <div class="header-logo">

                        </div>
                        <div class="logo-text"><span class="somespace">AUTO</span> MALL</div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-2 col-xs-2 col-sm-2 col-lg-2">
                        {{include('NumaDOASiteBundle::sidebarMenu.html.twig')}}
                        {{ render_esi(controller('NumaDOASiteBundle:Default:featuredAdd', { 'max': 5,'order':1 })) }}
                    </div>
                    <div class="col-md-10 col-xs-10 col-sm-10 col-lg-10">
                        {% block content %}
                        {% endblock %}
                    </div>
                </div>
                <div class="row">
                    <div class="footer col-md-12 col-xs-12 col-sm-12 col-lg-12">
                        <a class="footer-img"></a>
                        {% include 'NumaDOASiteBundle::copyright.html.twig' %}
                    </div>
                </div>
            </div> <!-- /container -->
        </body>
</html>


